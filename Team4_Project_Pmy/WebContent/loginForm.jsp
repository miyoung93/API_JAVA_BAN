<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<title>main</title>
	<style>
		
		img{
			width: 301.06px;
			height: 106px;
			margin: -15px;
		}
		
		a{
			text-decoration: none;
			color: #1877F2;
			font-size: 14px;
		}
		
		input:focus{
			outline:1px solid #1877F2;
		}
		
		a:hover {
    		text-decoration: underline;
		}
		
	</style>
	<script type="text/javascript">
		
	</script>
